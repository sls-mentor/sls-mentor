{
  "name": "@sls-mentor/aws-api",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "format-check": "prettier --check . ../../.prettierignore",
    "format-fix": "prettier --write . ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm clean && pnpm package-transpile && pnpm package-types && pnpm package-types-aliases",
    "package-transpile": "tsup",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- src",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests",
    "watch": "pnpm clean dist && concurrently 'pnpm:package-* --watch'"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.409.0",
    "@aws-sdk/client-apigatewayv2": "^3.409.0",
    "@aws-sdk/client-appsync": "^3.409.0",
    "@aws-sdk/client-backup": "^3.409.0",
    "@aws-sdk/client-cloudformation": "^3.409.0",
    "@aws-sdk/client-cloudfront": "^3.409.0",
    "@aws-sdk/client-cloudwatch": "^3.515.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.409.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.409.0",
    "@aws-sdk/client-dynamodb": "^3.409.0",
    "@aws-sdk/client-eventbridge": "^3.409.0",
    "@aws-sdk/client-iam": "^3.421.0",
    "@aws-sdk/client-lambda": "^3.409.0",
    "@aws-sdk/client-rds": "^3.409.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.409.0",
    "@aws-sdk/client-s3": "^3.409.0",
    "@aws-sdk/client-secrets-manager": "^3.409.0",
    "@aws-sdk/client-sesv2": "^3.409.0",
    "@aws-sdk/client-sfn": "^3.409.0",
    "@aws-sdk/client-sns": "^3.409.0",
    "@aws-sdk/client-sqs": "^3.409.0",
    "@aws-sdk/client-sts": "^3.409.0",
    "@aws-sdk/types": "^3.408.0",
    "@sls-mentor/arn": "workspace:^",
    "object-hash": "^3.0.0",
    "p-limit": "^3.1.0",
    "throttled-queue": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/object-hash": "^3.0.4",
    "@vitest/coverage-c8": "0.30.0",
    "concurrently": "^8.2.1",
    "dependency-cruiser": "^13.1.5",
    "eslint": "^8.49.0",
    "json-schema-to-ts": "^2.9.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "0.30.0"
  }
}
